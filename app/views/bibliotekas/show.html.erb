<div class="container">
  <div class="row justify-content-center">
    <h4 class="mt-4 mb-4 display-4">Show details of biblioteka <%= @biblioteka.id %></h4>
  </div>
  <div class="row justify-content-center">
    <table class="table">
      <tbody>
      <tr>
        <th>Name:</th>
        <td><%= @biblioteka.name %></td>
        <th>Address:</th>
        <td><%= @biblioteka.address %></td>
        <th>Phone:</th>
        <td><%= @biblioteka.phone %></td>


      </tr>
      </tbody>
    </table>
  </div>
  <%= link_to "Back to list", root_path %>
</div>
